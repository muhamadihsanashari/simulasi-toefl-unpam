<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simulasitoefl">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_simulasi_toefl_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_simulasi_toefl_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SimulasiTOEFL">
        <activity android:name=".practice.ReadingSectionActivity"></activity>
        <activity android:name=".practice.StructureSectionActivity" />
        <activity android:name=".practice.ListeningSectionActivity" />
        <activity android:name=".ScoresMenuActivity" />
        <activity android:name=".TestingMenuActivity" />
        <activity android:name=".PracticeMenuActivity" />
        <activity
            android:name=".ProfileActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>